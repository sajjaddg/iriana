<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="170.0" prefWidth="180.0" type="AnchorPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane fx:id="description_pane" prefHeight="170.0" prefWidth="180.0" style="-fx-background-color: #181C3A; -fx-background-radius: 6px;">
         <children>
            <Button fx:id="finalbutton" layoutX="-4.0" layoutY="130.0" mnemonicParsing="false" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="31.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icon/checkmark_52px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <TextArea fx:id="description" layoutX="17.0" layoutY="14.0" prefHeight="108.0" prefWidth="146.0" promptText="توضیحات " stylesheets="@style.css" />
            <TextField fx:id="expert" alignment="TOP_RIGHT" layoutX="37.0" layoutY="135.0" prefHeight="25.0" prefWidth="109.0" promptText="نام کارشناس" />
            <Button fx:id="returnbutton" layoutX="142.0" layoutY="129.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="26.0" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="31.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icon/undo_52px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </Pane>
      <Pane prefHeight="170.0" prefWidth="180.0" style="-fx-background-color: #181C3A; -fx-background-radius: 6px;">
         <children>
            <ImageView fx:id="icon_IV" fitHeight="61.0" fitWidth="62.0" layoutX="58.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="TrueButton" layoutX="135.0" layoutY="126.0" mnemonicParsing="false" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="38.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icon/thumbs_up_48px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="falsebutton" layoutX="3.0" layoutY="130.0" mnemonicParsing="false" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="39.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icon/thumbs_down_48px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <TextFlow fx:id="nameOfCompany" layoutX="42.0" layoutY="133.0" prefHeight="25.0" prefWidth="100.0" textAlignment="CENTER" />
            <Line endX="87.0" layoutX="55.0" layoutY="92.0" startX="-20.5" stroke="#2d314c" />
            <TextFlow fx:id="typeOfTest" layoutX="5.0" layoutY="95.0" prefHeight="25.0" prefWidth="171.0" textAlignment="CENTER" />
         </children></Pane>
   </children>
</fx:root>
